            "action": "run query",
            "identifier": "run_query",
            "description": "Run a query against SocialNet",
            "verbose": "Either an <b>id</b> or a combination of <b>parameter_type</b> and <b>parameter_value</b> must be supplied for the action to run.  The endpoint can and should include the placeholder for <b>id</b> substitution as {id} - for example: /twitter/users/{id}/followers or /instagram/posts/{id}/likers",
            "type": "investigate",
            "read_only": true,
            "parameters": {
                "endpoint": {
                    "description": "API endpoint to query (e.g. /facebook/users/{id}/photos)",
                    "data_type": "string",
                    "required": true,
                    "primary": false,
                    "default": "/search",
                    "order": 0
                },
                "id": {
                    "description": "ID of entity to query",
                    "data_type": "string",
                    "required": false,
                    "primary": true,
                    "contains": [
                        "socialnet entity id"
                    ],
                    "order": 1
                },
                "parameter_type": {
                    "description": "Type of parameter",
                    "data_type": "string",
                    "required": false,
                    "primary": false,
                    "value_list": [
                        "email",
                        "alias",
                        "phone",
                        "name",
                        "q"
                    ],
                    "order": 2
                },
                "parameter_value": {
                    "description": "Value for parameter to search",
                    "data_type": "string",
                    "required": false,
                    "primary": false,
                    "order": 3
                },
                "limit": {
                    "description": "Number of query results to return (max 10000)",
